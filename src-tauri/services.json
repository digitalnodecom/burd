{
  "services": {
    "meilisearch": {
      "display_name": "Meilisearch",
      "binary_name": "meilisearch",
      "default_port": 7700,
      "auto_create_domain": true,
      "health_check": {
        "type": "http",
        "path": "/health"
      },
      "config_fields": [
        {
          "key": "master_key",
          "label": "Master Key",
          "type": "password",
          "required": false
        }
      ],
      "start_args": [
        "--db-path", "{data_dir}",
        "--http-addr", "127.0.0.1:{port}",
        "--env", "development"
      ],
      "start_args_conditional": [
        {
          "if_config": "master_key",
          "args": ["--master-key", "{master_key}"]
        }
      ],
      "versions": {
        "source": "github_releases",
        "github_repo": "meilisearch/meilisearch"
      },
      "platforms": {
        "darwin-arm64": {
          "download": {
            "type": "github_asset",
            "asset_pattern": "meilisearch-macos-apple-silicon"
          },
          "is_archive": false
        },
        "darwin-x64": {
          "download": {
            "type": "github_asset",
            "asset_pattern": "meilisearch-macos-amd64"
          },
          "is_archive": false
        },
        "linux-x64": {
          "download": {
            "type": "github_asset",
            "asset_pattern": "meilisearch-linux-amd64"
          },
          "is_archive": false
        },
        "windows-x64": {
          "download": {
            "type": "github_asset",
            "asset_pattern": "meilisearch-windows-amd64.exe"
          },
          "is_archive": false,
          "binary_name": "meilisearch.exe"
        }
      }
    },

    "typesense": {
