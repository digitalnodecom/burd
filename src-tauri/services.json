          "key": "php_memory_limit",
          "label": "Memory Limit",
          "type": "text",
          "required": false,
          "default": "256M",
          "description": "PHP memory_limit setting for all parked sites"
        },
        {
          "key": "php_upload_max_filesize",
          "label": "Upload Max Size",
          "type": "text",
          "required": false,
          "default": "64M",
          "description": "PHP upload_max_filesize setting"
        },
        {
          "key": "php_post_max_size",
