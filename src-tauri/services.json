      },
      "config_fields": [
        {
          "key": "api_key",
          "label": "API Key",
          "type": "password",
          "required": false,
          "default": "xyz"
        }
      ],
      "start_args": [
        "--data-dir", "{data_dir}",
        "--api-port", "{port}",
        "--enable-cors",
        "--api-key", "{api_key}"
