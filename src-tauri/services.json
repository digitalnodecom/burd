        "v1.3.0": "PHP 8.3",
        "v1.2.5": "PHP 8.3",
        "v1.2.4": "PHP 8.3",
        "v1.2.3": "PHP 8.3",
        "v1.2.2": "PHP 8.3",
        "v1.2.1": "PHP 8.3",
        "v1.2.0": "PHP 8.3",
        "v1.1.5": "PHP 8.3",
        "v1.1.4": "PHP 8.3",
        "v1.1.3": "PHP 8.3",
        "v1.1.2": "PHP 8.3",
        "v1.1.1": "PHP 8.3",
        "v1.1.0": "PHP 8.3",
        "v1.0.0": "PHP 8.3"
      }
