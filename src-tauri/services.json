    "minio": {
      "display_name": "MinIO",
      "binary_name": "minio",
      "default_port": 9000,
      "auto_create_domain": true,
      "health_check": {
        "type": "http",
        "path": "/minio/health/live"
      },
      "config_fields": [
        {
          "key": "root_user",
          "label": "Root User",
          "type": "text",
          "required": false,
          "default": "minioadmin"
        },
        {
          "key": "root_password",
          "label": "Root Password",
          "type": "password",
          "required": false,
          "default": "minioadmin"
        }
      ],
      "start_args": [
        "server", "{data_dir}",
        "--address", "127.0.0.1:{port}",
        "--console-address", "127.0.0.1:{console_port}"
      ],
      "env_vars": [
        { "name": "MINIO_ROOT_USER", "value": "{root_user}" },
        { "name": "MINIO_ROOT_PASSWORD", "value": "{root_password}" }
      ],
      "computed_values": {
        "console_port": "{port} + 1"
      },
      "versions": {
        "source": "static",
        "versions": [
          "latest",
          "RELEASE.2024-12-18T13-15-44Z",
          "RELEASE.2024-11-07T00-52-20Z",
          "RELEASE.2024-10-02T17-50-41Z"
        ]
      },
      "platforms": {
        "darwin-arm64": {
          "download": {
            "type": "direct",
            "url_template": "https://dl.min.io/server/minio/release/darwin-arm64/minio",
            "url_template_versioned": "https://dl.min.io/server/minio/release/darwin-arm64/archive/minio.{version}"
          },
          "is_archive": false
        },
        "darwin-x64": {
          "download": {
            "type": "direct",
            "url_template": "https://dl.min.io/server/minio/release/darwin-amd64/minio",
            "url_template_versioned": "https://dl.min.io/server/minio/release/darwin-amd64/archive/minio.{version}"
          },
          "is_archive": false
        },
        "linux-x64": {
          "download": {
            "type": "direct",
            "url_template": "https://dl.min.io/server/minio/release/linux-amd64/minio",
            "url_template_versioned": "https://dl.min.io/server/minio/release/linux-amd64/archive/minio.{version}"
          },
          "is_archive": false
        },
        "windows-x64": {
          "download": {
            "type": "direct",
            "url_template": "https://dl.min.io/server/minio/release/windows-amd64/minio.exe",
            "url_template_versioned": "https://dl.min.io/server/minio/release/windows-amd64/archive/minio.{version}.exe"
