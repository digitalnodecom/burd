        },
        "darwin-x64": {
          "download": {
            "type": "github_asset",
            "asset_pattern": "frankenphp-mac-x86_64"
          },
          "is_archive": false
        },
        "linux-x64": {
          "download": {
            "type": "github_asset",
            "asset_pattern": "frankenphp-linux-x86_64"
          },
          "is_archive": false
        }
