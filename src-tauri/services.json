    "frankenphp": {
      "display_name": "PHP",
      "binary_name": "frankenphp",
      "default_port": 8000,
      "auto_create_domain": true,
      "health_check": {
        "type": "tcp"
      },
      "config_fields": [
        {
          "key": "document_root",
          "label": "Document Root",
          "type": "folder",
          "required": true,
          "description": "Path to PHP project directory"
        },
        {
          "key": "php_memory_limit",
          "label": "Memory Limit",
          "type": "text",
          "required": false,
          "default": "128M",
          "description": "PHP memory_limit setting"
        },
        {
          "key": "php_upload_max_filesize",
          "label": "Upload Max Size",
          "type": "text",
          "required": false,
          "default": "2M",
          "description": "PHP upload_max_filesize setting"
        },
        {
          "key": "php_post_max_size",
          "label": "Post Max Size",
          "type": "text",
          "required": false,
          "default": "8M",
          "description": "PHP post_max_size setting"
        },
        {
          "key": "php_max_execution_time",
          "label": "Max Execution Time",
          "type": "text",
          "required": false,
          "default": "30",
          "description": "PHP max_execution_time in seconds"
        }
      ],
      "start_args": [
        "php-server",
        "--listen", "127.0.0.1:{port}",
        "--root", "{document_root}"
      ],
      "env_vars": [
        { "name": "PHP_MEMORY_LIMIT", "value": "{php_memory_limit}" },
        { "name": "PHP_UPLOAD_MAX_FILESIZE", "value": "{php_upload_max_filesize}" },
        { "name": "PHP_POST_MAX_SIZE", "value": "{php_post_max_size}" },
        { "name": "PHP_MAX_EXECUTION_TIME", "value": "{php_max_execution_time}" }
      ],
      "versions": {
        "source": "github_releases",
        "github_repo": "dunglas/frankenphp"
