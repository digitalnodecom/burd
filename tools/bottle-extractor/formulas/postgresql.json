{
  "name": "postgresql",
  "homebrew_formula": "postgresql@17",
  "description": "Object-relational database system",
  "homepage": "https://www.postgresql.org/",
  "architectures": ["arm64", "x86_64"],
  "macos_versions": ["sonoma", "sequoia"],
  "binaries": [
    "postgres",
    "psql",
    "pg_dump",
    "pg_dumpall",
    "pg_restore",
    "createdb",
    "dropdb",
    "createuser",
    "dropuser",
    "pg_ctl",
    "initdb",
    "pg_isready",
    "pg_basebackup",
    "vacuumdb",
    "reindexdb",
    "clusterdb"
  ],
  "main_binary": "postgres",
  "dependencies": [
    {
      "formula": "openssl@3",
      "libs": ["libssl.3.dylib", "libcrypto.3.dylib"]
    },
    {
      "formula": "icu4c@78",
      "libs": []
    },
    {
      "formula": "lz4",
      "libs": ["liblz4.1.10.0.dylib", "liblz4.dylib", "liblz4.1.dylib"]
    },
    {
      "formula": "zstd",
      "libs": ["libzstd.1.5.7.dylib", "libzstd.1.dylib", "libzstd.dylib"]
    },
    {
      "formula": "krb5",
      "libs": []
    },
    {
      "formula": "readline",
      "libs": []
    },
    {
      "formula": "gettext",
      "libs": []
    }
  ],
  "test_commands": [
    "{main_binary} --version"
  ],
  "default_port": 5432,
  "notes": "PostgreSQL requires initialization with initdb before first use."
}
